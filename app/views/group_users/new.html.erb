<h1>Add User to Group</h1>
<%= form_with(model: [@group, @group_user], local: true) do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  <div>
    <%= form.label :admin %>
    <%= form.check_box :admin %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
